<section class="content-header">
  <h1> Distbutors <small class= "pull-right"><%= link_to 'New Distbutor', new_distbutor_path %></small></h1>
</section>
<div class="box">
  <!-- /.box-header -->
  <div class="box-body">
    <div class="row">
      <table id="example1" class="table table-bordered table-striped">
        <thead>
          <tr>
            <th>Name</th>
            <th colspan="2"></th>
          </tr>
        </thead>
        <tbody>
          <% @distbutors.each do |distbutor| %>
            <tr>
              <td><%= distbutor.name %></td>
              <td><%= link_to 'Show', distbutor %></td>
              <td><%= link_to 'Destroy', distbutor, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
          <% end %>
        </tbody>
        <tfoot>
          <tr>
            <th>Name</th>
            <th>Code</th>
            <th colspan="2"></th>
          </tr>
        </tfoot>
      </table>
    </div>
    <div class="row">
      <div class="col-sm-4"><%= page_entries_info @distbutors, entry_name: 'distbutor' %></div>
      <div class="col-sm-8 dataTables_paginate"><%= paginate @distbutors %></div>
    </div>
  </div>
  <!-- /.box-body -->
</div>
